<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Checkout Suite" verbose="1">
    <listeners>
        <listener class-name="com.automation.listeners.TestListener"/>
        <listener class-name="com.automation.listeners.AnnotationTransformer"/>
        <listener class-name="com.automation.listeners.MethodInterceptor"/>
    </listeners>

    <test name="Checkout Tests">
        <classes>
            <class name="com.automation.ui.checkout.CheckoutTest"/>
        </classes>
    </test>
</suite>
